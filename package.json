    "concurrently": "^4.1.0",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "generate-schema": "^2.6.0",
    "mongoose": "^5.3.12",
    "nodemon": "^1.18.6",
    "passport": "^0.4.0",
